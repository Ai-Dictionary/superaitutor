<script nonce="{{nonce}}">
async function getLegalContent(viewId = 0){
    try{
        const response = await fetch('/security', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'Accept': 'text/html'
            },
            body: JSON.stringify({ view: viewId })
        });

        if (!response.ok) {
            throw new Error(`Server responded with status ${response.status}`);
        }

        const htmlContent = await response.text();
        document.querySelector('main').innerHTML = htmlContent;
    }catch(error){
        console.error('Error fetching legal content:', error);
        system.alert({'error': 500, 'message': "Failed to load the security content due to some unexpted error. Please try again some time later."})
    }
}

(async ()=> {
    const urlParams = new URLSearchParams(window.location.search);
    const viewParam = urlParams.get('view')*1 || 0;
    await getLegalContent(viewParam);
})();
</script>
